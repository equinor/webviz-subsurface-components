{
    "name": "@webviz/subsurface-components",
    "description": "Custom Dash components for use in Webviz",
    "repository": {
        "type": "git",
        "url": "git://github.com/equinor/webviz-subsurface-components.git"
    },
    "bugs": {
        "url": "https://github.com/equinor/webviz-subsurface-components/issues"
    },
    "homepage": "https://github.com/equinor/webviz-subsurface-components",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist/**/*"
    ],
    "scripts": {
        "start": "webpack serve --mode development --devtool inline-source-map --entry ./src/demo/index.js --open",
        "typecheck": "tsc --noEmit --project tsconfig.eslint.json",
        "lint": "eslint *.js *.json \"**/*.{js,jsx,ts,tsx,mdx,json}\" .storybook",
        "format": "npm run lint -- --fix",
        "validate": "npm run typecheck && npm run lint",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "storybook:test": "test-storybook --no-index-json",
        "docker:build": "docker build --tag wsc_story_test --file Dockerfile.test .",
        "docker:storybook:test": "cross-env-shell PWD=$PWD npm run docker:storybook:test:docker:core -- -it --rm --mount type=bind,source=${PWD}/packages,target=/test/packages --network=host wsc_story_test",
        "docker:storybook:test:update": "npm run docker:storybook:test -- npm run storybook:test -- --url http://host.docker.internal:6006/ --no-index-json --updateSnapshot",
        "docker:storybook:test:docker:core": "docker run",
        "cy:run": "npx cypress run -b chrome --component --headed",
        "postpack": "npm run tidy-up",
        "git:hooks:enable": "cd .. && husky install",
        "git:hooks:disable": "cd .. && husky uninstall",
        "prepare": "cd .. && husky install || exit 0",
        "prepublishOnly": "npm test && npm run lint",
        "preversion": "npm run lint"
    },
    "browserslist": [
        "last 1 version",
        "> 1%",
        "IE 10"
    ],
    "author": "Equinor <opensource@equinor.com>",
    "license": "MPL",
    "devDependencies": {
        "@actions/core": "^1.10.1",
        "@babel/preset-react": "^7.24.7",
        "@cypress/react18": "^2.0.1",
        "@mdx-js/react": "^3.0.1",
        "@mui/material": "^5.15.15",
        "@mui/system": "^5.15.14",
        "@nx/jest": "^19.2.3",
        "@storybook/addon-essentials": "^8.3.4",
        "@storybook/addon-interactions": "^8.3.4",
        "@storybook/addon-links": "^8.3.4",
        "@storybook/addon-storysource": "^8.3.4",
        "@storybook/addon-webpack5-compiler-babel": "^3.0.3",
        "@storybook/blocks": "^8.3.0",
        "@storybook/react": "^8.3.4",
        "@storybook/react-webpack5": "^8.3.4",
        "@storybook/test": "^8.3.0",
        "@storybook/test-runner": "^0.19.1",
        "@testing-library/jest-dom": "^6.5.0",
        "@testing-library/react": "^16.0.1",
        "@testing-library/user-event": "^14.5.2",
        "@theunderscorer/nx-semantic-release": "^2.11.0",
        "@types/convert-units": "^2.3.9",
        "@types/d3": "^7.4.3",
        "@types/d3-color": "^3.1.3",
        "@types/d3-interpolate": "^3.0.4",
        "@types/geojson": "^7946.0.14",
        "@types/jest": "^29.5.12",
        "@types/lodash": "^4.17.5",
        "@types/react": "^18.3.11",
        "@types/react-dom": "^18.3.0",
        "@types/react-redux": "^7.1.34",
        "@types/uuid": "^8.3.0",
        "@types/workerpool": "^6.4.7",
        "@typescript-eslint/eslint-plugin": "^8.8.0",
        "@typescript-eslint/parser": "^8.8.0",
        "assert": "^2.0.0",
        "babel-jest": "^29.6.0",
        "copyfiles": "^2.0.0",
        "cross-env": "^7.0.3",
        "css-loader": "^5.2.0",
        "cypress": "^13.13.0",
        "cypress-image-diff-js": "^2.1.4",
        "eslint": "^8.53.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.30.0",
        "eslint-plugin-mdx": "^3.1.5",
        "eslint-plugin-prettier": "^5.2.1",
