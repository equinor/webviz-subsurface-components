        "vue-style-loader": "^4.1.3"
    },
    "peerDependencies": {
        "react": "^17 || ^18",
        "react-dom": "^17 || ^18"
    },
    "engines": {
        "node": ">=14.17",
        "npm": ">=6.14"
    },
    "volta": {
        "node": "18.19.0"
    },
    "workspaces": [
        "packages/*"
    ]
}
