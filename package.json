{
    "name": "webviz_subsurface_components",
    "description": "Custom Dash components for use in Webviz",
    "main": "build/index.js",
    "scripts": {
        "start": "webpack-serve ./webpack.serve.config.js --open",
        "postinstall": "cp ./package.json webviz_subsurface_components/package.json",
        "validate-init": "python _validate_init.py",
        "prepublishOnly": "npm run validate-init",
        "build:js-dev": "webpack --mode development",
        "build:js": "webpack --mode production",
        "build:py": "dash-generate-components ./src/lib/components webviz_subsurface_components",
        "build:all": "npm run build:js && npm run build:js-dev && npm run build:py",
        "test": "jest",
        "format": "prettier --write *.js *.json 'src/**/*.+(js|jsx|json|css)'",
        "linting": "prettier --check *.js *.json 'src/**/*.+(js|jsx|json|css)'"
    },
    "author": "R&T Equinor",
    "license": "LGPL",
    "dependencies": {
        "@material-ui/core": "^4.3.3",
        "d3": "^5.11.0",
        "leaflet": "^1.5.1",
        "leaflet-draw": "^1.0.4",
        "ramda": "^0.25.0",
        "react-leaflet": "^2.4.0",
        "react-leaflet-draw": "^0.19.0"
    },
    "devDependencies": {
        "@babel/core": "^7.5.5",
        "@babel/preset-env": "^7.5.5",
        "@babel/preset-react": "^7.0.0",
        "babel-eslint": "^9.0.0",
        "babel-loader": "^8.0.0",
        "chai": "^4.2.0",
        "copyfiles": "^2.0.0",
        "css-loader": "^2.1.1",
        "eslint": "^5.16.0",
        "eslint-config-prettier": "^4.3.0",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-react": "^7.14.3",
        "jest": "^24.9.0",
        "jsverify": "^0.8.4",
        "npm": "^6.11.2",
        "prettier": "^1.18.2",
        "raw-loader": "^3.1.0",
        "react": "^16.9.0",
        "react-docgen": "^2.20.1",
        "react-dom": "^16.9.0",
        "style-loader": "^0.21.0",
        "url-loader": "^2.1.0",
        "webpack": "^4.39.3",
        "webpack-cli": "^3.3.7",
        "webpack-serve": "^1.0.2"
    },
    "engines": {
        "node": ">=8.11.0",
        "npm": ">=6.1.0"
    }
}
